Cell[TextData[{
 "(a) Generate a million random entries in [0, 5] \[Times][0,5] for \[Beta] = \
",
 Cell[BoxData[
  FormBox[
   RowBox[{"[", 
    RowBox[{
     SubscriptBox["\[Beta]", "0"], ",", " ", 
     SubscriptBox["\[Beta]", "1"]}]}], TraditionalForm]],ExpressionUUID->
  "fe9db334-10b6-4b45-b73d-84c76e9c645c"],
 "]\[Transpose], and estimate \[Beta] which minimises ||A\[Beta] - y||.\nThe \
function lineFit[ ] below performs this simulation."
}], "Text",
 CellChangeTimes->{{3.824452938844429*^9, 3.824452991858005*^9}, {
  3.82445303190224*^9, 3.8244531428269672`*^9}, {3.8245902787520514`*^9, 
  3.8245903044703007`*^9}},
 FontSize->16,ExpressionUUID->"8a05cd60-713d-407b-9e99-6e1061a02079"],

Cell[BoxData[
 RowBox[{
  RowBox[{"lineFit", "[", "nrun_", "]"}], ":=", " ", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"beta", ",", " ", "error", ",", " ", "norm", ",", " ", 
      RowBox[{"n", "=", "nrun"}], ",", " ", "est"}], "}"}], ",", 
    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"beta", " ", "=", " ", 
      RowBox[{"RandomReal", "[", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{"0", ",", "5"}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{"2", ",", "n"}], "}"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"norm", " ", "=", 
      RowBox[{"ConstantArray", "[", 
       RowBox[{"0", ",", "n"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"For", "[", 
      RowBox[{
       RowBox[{"i", "=", "1"}], ",", " ", 
       RowBox[{"i", "<", " ", 
        RowBox[{"n", "+", "1"}]}], ",", 
       RowBox[{"i", "++"}], ",", " ", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"error", " ", "=", 
         RowBox[{
          RowBox[{"A", ".", 
           RowBox[{"beta", "[", 
            RowBox[{"[", 
             RowBox[{"All", ",", "i"}], "]"}], "]"}]}], "-", "y"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"norm", " ", "[", 
          RowBox[{"[", "i", "]"}], "]"}], "=", " ", 
         RowBox[{"Norm", "[", "error", "]"}]}], ";"}]}], 
      "\[IndentingNewLine]", 
      RowBox[{"(*", 
       RowBox[{
        RowBox[{"Print", "[", "error", "]"}], ";"}], "*)"}], "  ", 
      RowBox[{"(*", " ", "comment", "*)"}], "\[IndentingNewLine]", "]"}], ";",
      "\[IndentingNewLine]", "          ", 
     RowBox[{"(*", 
      RowBox[{
       RowBox[{"Print", "[", "norm", "]"}], ";"}], "*)"}], 
     "\[IndentingNewLine]", "\t", 
     RowBox[{"min", " ", "=", " ", 
      RowBox[{"Min", "[", "norm", "]"}]}], ";", " ", 
     RowBox[{"Print", "[", "min", "]"}], ";", "\[IndentingNewLine]", "\t", 
     RowBox[{"indx", " ", "=", " ", 
      RowBox[{
       RowBox[{"Position", "[", 
        RowBox[{"norm", ",", "min"}], "]"}], "[", 
       RowBox[{"[", 
        RowBox[{"1", ",", "1"}], "]"}], "]"}]}], ";", " ", 
     RowBox[{"Print", "[", "indx", "]"}], ";", "\[IndentingNewLine]", "\t", 
     RowBox[{"(*", 
      RowBox[{
       RowBox[{"Print", "[", "beta", "]"}], ";"}], "*)"}], 
     "\[IndentingNewLine]", "         ", 
     RowBox[{"beta", "[", 
      RowBox[{"[", 
       RowBox[{"All", ",", "indx"}], "]"}], "]"}]}]}], "\[IndentingNewLine]", 
   "]"}]}]], "Input",
